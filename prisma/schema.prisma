generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model ParkingSpot {
  id           Int     @id @default(autoincrement())
  name         String
  latitude     Float
  longitude    Float
  people       String
  vehicleType  String
  capacity     Int
  vacant       Int
  updatedAt    DateTime @updatedAt
}

model Conjestion {
  id              Int      @id @default(autoincrement())
  checkPointName String
  inFlow          Int
  outFlow         Int
  netFlow         Int
  thresold        Int
  colorCode       String
  status          String
  timestamp       DateTime @updatedAt
}